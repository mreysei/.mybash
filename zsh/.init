#!/bin/bash

export ZSH="$MYBASH/zsh/original"
zshVersion=`zsh --version 2>&1 > /dev/null`

if [[ "$zshVersion" == *"not found"* ]]; then
  echo "Asign zsh by default shell..."
  chsh -s $ZSH
fi

# See https://github.com/ohmyzsh/ohmyzsh/wiki/Themes
ZSH_THEME="skaro" 

plugins=(git)

source $ZSH/oh-my-zsh.sh
